{
  "name": "jkurwa",
  "version": "1.21.2",
  "description": "Ukrainian national EC cryptography (DSTU-4145) implementaion",
  "type": "module",
  "main": "dist/index.js",
  "browser": {
    "./lib/rand.js": "./lib/rand.browser.js"
  },
  "jest": {
    "testRegex": [
      "test/test.*.js"
    ],
    "setupFiles": [
      "./test/setup.js"
    ],
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "cover": "npm run test -- --coverage",
    "build": "esbuild ./lib/index.js --outdir=./dist/ --bundle --platform=node"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dstucrypt/jkurwa"
  },
  "keywords": [
    "ecc",
    "cryptography",
    "dstu4145",
    "ukraine"
  ],
  "author": "Ilya Petrov",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/dstucrypt/jkurwa/issues"
  },
  "homepage": "https://github.com/dstucrypt/jkurwa/",
  "devDependencies": {
    "codecov": "^3.8.1",
    "esbuild": "^0.25.5",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gost89": "^0.1.11",
    "jest": "29.x",
    "prettier": "^1.17.0"
  },
  "dependencies": {
    "asn1.js": "muromec/asn1.js",
    "bn.js": ">= 0.14",
    "buffer": ">= 3.0",
    "jksreader": "^1.0.0",
    "js-lzma": "0.x"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ]
  }
}
